<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <AssemblyName>Hyperledger.Indy.Sdk</AssemblyName>
    <RootNamespace>Hyperledger.Indy.Sdk</RootNamespace>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Symon Rottem</Authors>
    <Company>Hyperledger Indy</Company>
    <Product>Indy.Sdk.DotNet</Product>
    <Description>.NET wrapper for the Indy SDK</Description>
    <PackageProjectUrl>https://github.com/hyperledger/indy-sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/hyperledger/indy-sdk</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.1\Indy.Sdk.DotNet.xml</DocumentationFile>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors></WarningsAsErrors>
  </PropertyGroup>
  
</Project>